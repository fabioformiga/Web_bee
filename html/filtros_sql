SELECT * FROM `medida` WHERE data >= (DATE_SUB(CURDATE(), INTERVAL 1 YEAR)) 
SELECT * FROM `medida` WHERE data >= (DATE_SUB(CURDATE(), INTERVAL 1 MONTH)) 
SELECT * FROM `medida` WHERE data >= (DATE_SUB(CURDATE(), INTERVAL 1 WEEK)) 
SELECT * FROM `medida` WHERE data >= (DATE_SUB(CURDATE(), INTERVAL 1 DAY)) 
